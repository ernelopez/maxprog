<h1>Listing posts</h1>

<table>
  <tr>
    <th>Id</th>
    <th>Obra</th>
    <th>Autor1</th>
    <th>Autor2</th>
    <th>Autor3</th>
    <th>Autor4</th>
    <th>Fecha</th>
    <th>File</th>
    <!-- <th>Codigo</th> -->
    <th>EscuelaId</th>
    <th>Escuela</th>
    <th>Curso</th>
    <th>Provincia</th>
    <th>DocenteId</th>
    <th>Docente</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @posts.each do |post| %>
  <tr>
    <td><%= post.id %></td>
    <td><%= post.obra %></td>
    <td><%= post.autor1 %></td>
    <td><%= post.autor2 %></td>
    <td><%= post.autor3 %></td>
    <td><%= post.autor4 %></td>
    <td><%= post.fecha %></td>
    <td><%= post.file %></td>
    <!-- <td><%= post.codigo %></td>  -->
    <td><%= post.escuela_id %></td>
    <td><%= post.escuela.institu %></td>
    <td><%= post.curso %></td>
    <td><%= post.escuela.provi.prov %></td>
    <td><%= post.docente_id %></td>
    <td><%= post.docente.name %></td>
    <td><%= link_to 'Show', post %></td>
    <td><%= link_to 'Edit', edit_post_path(post) %></td>
    <td><%= link_to 'Destroy', post, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Post', new_post_path %>
