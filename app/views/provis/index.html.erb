<h1>Listing provis</h1>

<table>
  <tr>
    <th>Id</th>
    <th>Prov</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @provis.each do |provi| %>
  <tr>
    <td><%= provi.id %></td>
    <td><%= provi.prov %></td>
    <td><%= link_to 'Show', provi %></td>
    <td><%= link_to 'Edit', edit_provi_path(provi) %></td>
    <td><%= link_to 'Destroy', provi, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

